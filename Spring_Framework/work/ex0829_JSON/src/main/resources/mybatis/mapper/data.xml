<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="data">
    <insert id="add" parameterType="mybatis.vo.DataVO">
        insert into memo_t(writer, content, ip,reg_date)
        values(#{writer}, #{content}, #{ip}, now())
    </insert>
</mapper>