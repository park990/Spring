<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sist.ex0916_memo.mapper.MemoMapper">
  <select id="all" resultType="com.sist.ex0916_memo.vo.MemoVO">
    select * from memo_t
  </select>
  <select id="getMemo" resultType="com.sist.ex0916_memo.vo.MemoVO" parameterType="String">
    select * from memo_t where idx=#{idx}
  </select>
</mapper>